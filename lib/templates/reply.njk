---
date: {{ published }}
in-reply-to: {{ inReplyTo }}
link: {{ inReplyTo }}
{%- if rsvp %}
rsvp: {{ rsvp }}
{%- endif %}
category:
- "social"
- "Reply"
{%- if category %}
tag:
{%- for item in category %}
- {{ item }}
{%- endfor %}
{%- endif %}
{%- if syndicateTo %}
syndicate-to:
{%- for item in syndicateTo %}
- {{ item }}
{%- endfor %}
{%- endif %}
---
{{ content | safe }}
